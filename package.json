{
  "name": "your-gaming-toolkit",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "schema": "app/(features)/database/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next dev -p 3000 --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "env": "vercel env pull .env",
    "check-types": "tsc --noEmit",
    "db:migrate": "pnpm exec prisma migrate dev",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@mantine/carousel": "^7.15.0",
    "@mantine/charts": "^7.15.0",
    "@mantine/code-highlight": "^7.15.0",
    "@mantine/core": "^7.15.0",
    "@mantine/dates": "^7.15.0",
    "@mantine/dropzone": "^7.15.0",
    "@mantine/form": "^7.15.0",
    "@mantine/hooks": "^7.15.0",
    "@mantine/modals": "^7.15.0",
    "@mantine/notifications": "^7.15.0",
    "@mantine/nprogress": "^7.15.0",
    "@mantine/spotlight": "^7.15.0",
    "@mantine/tiptap": "^7.15.0",
    "@prisma/client": "^6.1.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@tabler/icons-react": "^3.24.0",
    "@tiptap/extension-link": "^2.10.3",
    "@tiptap/pm": "^2.10.3",
    "@tiptap/react": "^2.10.3",
    "@tiptap/starter-kit": "^2.10.3",
    "@vercel/flags": "^2.6.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^7.1.0",
    "js-cookie": "^3.0.5",
    "next": "15.1.2",
    "next-auth": "5.0.0-beta.25",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-redux": "^9.2.0",
    "recharts": "2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "prisma": "6.1.0",
    "typescript": "5.5.4"
  }
}
